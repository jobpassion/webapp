    <insert id="insert-note-coppy">
INSERT
INTO
    notes
    (
        user_id,
        title,
        DESC,
        url,
        list,
        tag,
        updated,
        created
    )
    VALUES
    (
        #userId#,
        #title#,
        #desc#,
        #url#,
        #list#,
        #tag#,
        #updated#,
        #created#
    )
    </insert>
    <insert id="insert-note">
INSERT
INTO
    chrome_note.notes
    (
        user_id,
        title,
        `DESC`,
        url,
        list,
        tag,
        updated,
        created
    )
    VALUES
    (
        #userId#,
        #title#,
        #desc#,
        #url#,
        #list#,
        #tag#,
        #updated#,
        #created#
    )
    </insert>
    <update id="update-note">
        UPDATE chrome_note.notes SET 
        <isNotEmpty property="title" prepend=", ">title=#title#</isNotEmpty>
        <isNotEmpty property="desc" prepend=", ">title=#desc#</isNotEmpty>
        <isNotEmpty property="url" prepend=", ">title=#url#</isNotEmpty>
        <isNotEmpty property="list" prepend=", ">title=#list#</isNotEmpty>
        <isNotEmpty property="tag" prepend=", ">title=#tag#</isNotEmpty>
        id = id
    </update>
</sqlMap>